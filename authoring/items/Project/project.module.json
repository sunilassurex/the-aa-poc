{
  "$schema": "../../.sitecore/schemas/ModuleFile.schema.json",
  "namespace": "Project.TheAA",
  "references": ["Feature.TheAA"],
  "items": {
    "includes": [
      {
        "name": "MobileAppProject",
        "path": "/sitecore/templates/Project/AA",
        "allowedPushOperations": "CreateUpdateAndDelete"
      },
      {
        "name": "renderings",
        "path": "/sitecore/layout/Renderings/Project/AA",
        "allowedPushOperations": "CreateUpdateAndDelete"
      },
      {
        "name": "LayoutMobileApp",
        "path": "/sitecore/layout/Layouts/Project/mobile-app",
        "allowedPushOperations": "CreateUpdateAndDelete"
      },
      {
        "name": "placeholdersettings",
        "path": "/sitecore/layout/Placeholder Settings/Project/AA",
        "allowedPushOperations": "CreateUpdateAndDelete"
      },
      {
        "name": "mediaMobileApp",
        "path": "/sitecore/media library/mobile-app",
        "scope": "ItemAndDescendants",
        "allowedPushOperations": "CreateOnly"
      },
      {
        "name": "tenantfolder",
        "path": "/sitecore/content/AA",
        "scope": "SingleItem",
        "allowedPushOperations": "CreateAndUpdate"
      },
      {
        "name": "mobileapp",
        "path": "/sitecore/content/AA/mobile-app",
        "allowedPushOperations": "CreateOnly",
        "rules": [
          {
            "path": "/mobile-app",
            "allowedPushOperations": "CreateOnly",
            "scope": "ItemAndDescendants"
          },
          {
            "path": "/Media",
            "scope": "SingleItem",
            "allowedPushOperations": "CreateOnly"
          },
          {
            "path": "/Dictionary",
            "scope": "SingleItem",
            "allowedPushOperations": "CreateOnly"
          },
          {
            "path": "/Data",
            "scope": "ItemAndDescendants",
            "allowedPushOperations": "CreateOnly"
          },
          {
            "path": "/Presentation",
            "scope": "ItemAndDescendants",
            "allowedPushOperations": "CreateOnly"
          },
          {
            "path": "/Settings",
            "scope": "ItemAndDescendants",
            "allowedPushOperations": "CreateOnly"
          },
          {
            "path": "*",
            "scope": "Ignored"
          }
        ]
      }
    ]
  }
}
